<p id="notice"><%= notice %></p>

<h1>Listing Engine Tags</h1>

<table>
  <thead>
    <tr>
      <th>Engine</th>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @engine_tags.each do |engine_tag| %>
      <tr>
        <td><%= engine_tag.engine_id %></td>
        <td><%= engine_tag.tag.name %></td>
        <td><%= link_to 'Show', engine_tag %></td>
        <td><%= link_to 'Edit', edit_engine_tag_path(engine_tag) %></td>
        <td><%= link_to 'Destroy', engine_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Engine tag', new_engine_tag_path %>
