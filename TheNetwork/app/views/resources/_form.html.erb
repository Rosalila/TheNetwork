<%= form_for(@resource) do |f| %>
  <% if @resource.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@resource.errors.count, "error") %> prohibited this resource from being saved:</h2>

      <ul>
      <% @resource.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <label class="col-sm-2 col-sm-2 control-label"><%= f.label :name %></label>
    <div class="col-sm-10">
      <%= f.text_field :name, :class => "form-control" %><br>
    </div>
  </div>

  <div class="field">
    <label class="col-sm-2 col-sm-2 control-label"><%= f.label :image %></label>
    <div class="col-sm-10">
      <%= f.file_field :image %><br>
    </div>
  </div>

  <div class="field">
    <label class="col-sm-2 col-sm-2 control-label"><%= f.label :component_id %></label>
    <div class="col-sm-10">
      <%= f.number_field :component_id, :class => "form-control" %><br>
    </div>
  </div>

  <div class="field">
    <label class="col-sm-2 col-sm-2 control-label"><%= f.label :category_id %></label>
    <div class="col-sm-10">
      <%= f.number_field :category_id, :class => "form-control" %><br>
    </div>
  </div>

  <div class="field">
    <label class="col-sm-2 col-sm-2 control-label"><%= f.label :download %></label>
    <div class="col-sm-10">
      <%= f.file_field :download %><br>
    </div>
  </div>

  <div class="field">
    <label class="col-sm-2 col-sm-2 control-label"><%= f.label :license_id %></label>
    <div class="col-sm-10">
      <%= f.text_field :license_id, :class => "form-control" %><br>
    </div>
  </div>

  <div class="field">
   <label class="col-sm-2 col-sm-2 control-label"> <%= f.label :video %></label>
   <div class="col-sm-10">
      <%= f.text_field :video, :class => "form-control" %><br>
    </div>
  </div>

  <div class="actions">
    <%= f.submit :class => "btn btn-success" %>
    <button  class = "btn btn-theme"><%= link_to 'Back', :back %></button>
  </div>
<% end %>
